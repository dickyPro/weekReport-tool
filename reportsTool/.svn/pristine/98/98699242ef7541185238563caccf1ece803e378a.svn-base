<style type="text/css">
	button
	{
		background:none;
		border:0;
	}
	.div_paddingTop_10
	{
		padding-left:5px;
		padding-top:5px;
	}
	.input_no_border{
	width:95%;
	border-left:0px;
	border-top:0px;
	border-right:0px;
	border-bottom:1px:
	background:#F0F0F0;
	border-style:solid;
	border-color:#eaeaea;
	padding-top:20px;
	}
</style>

<script type="text/javascript">
    $(document).ready(function (){
    		var jsconfig="";
			$.post("../outputReport/queryReportTimes",{},
		 		function(result){
		 		 var data = eval('(' + result + ')');
			 		if(data._ajax_op_flag== "success"){
			 			var res=data._ajax_data;
			 			var mySelect= document.getElementById("seltime");
			 			mySelect.add(new Option("",""));	
				 		for(var i=0;i<res.length;i++){						
						    var opp = new Option(res[i],res[i]);
                            opp.name = res[i].name;
                            //将option添加到select标签里面
                            mySelect.add(opp);						   
						}				
			         }
			      }
			);
    })
	function submitReportsdata(){
	  
	      var time= document.getElementById("seltime");
          if (time==null||time.value=="")
          {
             alert("请选择期数!")
             return ;
          }  
	  
	   $("#reportDataForm").attr("action","../outputReport/outputReportsInfo");
	   $("#reportDataForm").submit();  
	   //var data = eval('(' + result + ')');			 		
	   //var res=data._ajax_data;
	   //if(res!="")
	   //{
	   //window.location.href='loadReports.html';
         //window.open("../outputfile/weekreportFile.xls","","width=600,height=300");
       //}
	}
	
</script>




  </head>
  
  <body>

  <form id="reportDataForm" action="../outputReport/outputReportsInfo" data-ajax="false" method="post" >
   	
   	 <div style="margin-left: auto;margin-right: auto ;margin-top: 10px;text-align: center;"> 
         <p>
         <label>选择周报期数:</label>
         </p>
	          <select id="seltime" name="seltime" style="width:280px;height:25px;border:1px rgba(0, 0, 0, 0.2) solid;" class="easyui-combobox" name="dept";"></select>                                    
     </div> 
   	
   				    
   <div class="form_Reportbutton" onMouseOver="this.style.cursor='pointer'" onClick="submitReportsdata();">
			 <label style="color: white;font-weight: bold;font-size: 12pt;">周报导出</label>
   </div>
 
 </form>
  </body>
</html>
