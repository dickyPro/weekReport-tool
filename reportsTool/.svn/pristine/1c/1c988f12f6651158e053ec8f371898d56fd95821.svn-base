package com.gtmap.platform.jdbc;

public class JdbcDaoSupportEx {
	/**
	 * 注入数据库操作类
	 */
	public JdbcTemplateEx  jdbcTemplateEx;

	/**
	 * 注入数据库模式名
	 */
	public String tableSchema= ""; 	
	
	/**
	 * @return the jdbcTemplateEx
	 */
	public JdbcTemplateEx getJdbcTemplateEx() {
		return jdbcTemplateEx;
	}

	/**
	 * @param jdbcTemplateEx the jdbcTemplateEx to set
	 */
	public void setJdbcTemplateEx(JdbcTemplateEx jdbcTemplateEx) {
		this.jdbcTemplateEx = jdbcTemplateEx;
	}
	
	/**
	 * @return the tableSchema
	 */
	public String getTableSchema() {
		return tableSchema;
	}
	/**
	 * @param tableSchema the tableSchema to set
	 */
	public void setTableSchema(String tableSchema) {
		this.tableSchema = tableSchema;
	}	
}
